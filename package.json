{
  "devDependencies": {
    "esbuild": "^0.18.17",
    "typescript": "^5.1.6"
  },
  "name": "js-music",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "npx concurrently -n 'build,serve' 'npm run watch' 'npm run serve'",
    "watch-index": "npx esbuild src/index.ts --bundle --outfile=dist/index.js --format=esm --watch --minify --sourcemap",
    "watch-tests": "npx esbuild src/tests.ts --bundle --outfile=dist/tests.js --format=esm --watch --minify --sourcemap",
    "watch": "npx concurrently -n 'index,tests' 'npm run watch-index' 'npm run watch-tests'",
    "serve": "npx http-server --cors --port 3000 --c-1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximefabas/js-music.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maximefabas/js-music/issues"
  },
  "homepage": "https://github.com/maximefabas/js-music#readme",
  "description": ""
}
