{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "DOM",
      "ESNext",
      "ES2022"
    ],
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "module": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true
  },
}
